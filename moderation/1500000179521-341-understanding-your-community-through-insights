<script>
function showActiveArticle(){
  let moderationCategoriesContainer = document.getElementById('dropdown-moderation-wrapper');
  if (moderationCategoriesContainer) {
    openActiveCategory(moderationCategoriesContainer);
  }
}

function openActiveCategory(moderationCategoriesContainer) {

  let categories = document.querySelectorAll('.w-dropdown');
  for(let category of categories) {
    let activeArticles = category.querySelectorAll('.w--current');
    if (activeArticles.length > 0) {
      let categoryTitle = category.querySelector('.w-dropdown-toggle');
      if (categoryTitle) {
        console.log('categoryTitle', categoryTitle);
        triggerClickEvent(categoryTitle, 'mousedown');
        triggerClickEvent(categoryTitle, 'mouseup');
      //   categoryTitle.classList.add('w--open');
      //   categoryTitle.ariaExpanded = 'true';
      // }
      //
      // let categoryList = category.querySelector('.w-dropdown-list');
      // if (categoryList) {
      //   categoryList.classList.add('w--open');
      }
    }
  }
}

function triggerClickEvent (node, eventType) {
  var clickEvent = document.createEvent('MouseEvents');
  clickEvent.initEvent(eventType, true, true);
  node.dispatchEvent(clickEvent);
}

showActiveArticle();
</script>
